<form action="/create_bookmark_from_dish" method="post">

  <input type="hidden" name="user_id" value="<%= current_user.id %>">

  <div class="form-group">
    <label for="venues_id">
      Venues
    </label>

    <%= select_tag(:venues_id, options_from_collection_for_select(Venue.all, :id, :cuisine, @bookmark.venues_id), :class => "form-control") %>
  </div>

  <input type="hidden" name="dish_id" value="<%= @dish.id %>">

  <button class="btn btn-block btn-outline-secondary">
    Create bookmark
  </button>
</form>
